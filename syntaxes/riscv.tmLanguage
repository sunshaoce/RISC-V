          vfclass\.v |
          vfcvt\.f\.xu?\.v |
          vfcvt\.rtz\.xu?\.f\.v |
          vfcvt\.xu?\.f\.v |
          vfdiv\.v[fv] |
          vfirst(\.m)? |
