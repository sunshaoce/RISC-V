          vm?sbc\.v[vx]?m? |
          vmacc\.v[vx] |
          vmadd\.v[vx] |
          vmandn?\.mm |
          vmandnot\.mm |
          vmaxu?\.v[vx] |
          vmclr\.m |
          vmerge\.v[ivx]m? |
          vmfeq\.v[fv] |
          vmfg[te]\.v[fv] |
          vmfl[et]\.v[fv] |
