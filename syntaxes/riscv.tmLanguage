          vfmv\.v\.f |
          vfn?macc\.v[fv] |
          vfn?madd\.v[fv] |
          vfn?msac\.v[fv] |
          vfn?msub\.v[fv] |
          vfncvt\.f\.[fx]\.w |
          vfncvt\.f\.xu?\.w |
          vfncvt\.rod\.f\.f\.w |
          vfncvt\.rtz\.xu?\.f\.w |
          vfncvt\.xu?\.f\.w |
          vfneg\.v |
          vfrdiv\.vf |
          vfrec7\.v |
          vfwredsum\.vs |
          vfredmax\.v[sv] |
          vfredmin\.v[sv] |
          vfredosum\.v[sv] |
          vfredusum\.v[sv] |
          vfredsum\.vs |
          vfrsqrt7\.v |
          vfrsub\.vf |
          vfsgnj[nx]?\.v[fv] |
          vfslide1down\.vf |
          vfslide1up\.vf |
          vfsqrt\.v |
          vfsub\.v[fv] |
          VFUNARY[01]\.vv |
